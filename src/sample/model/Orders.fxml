<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.model.Orders">
    <children>
        <Button fx:id="closing" layoutX="163.0" layoutY="297.0" mnemonicParsing="false" onAction="#close" prefHeight="50.0" prefWidth="120.0" style="-fx-background-color: #ff0000;" text="Отмена">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Button>
        <Button fx:id="adding" layoutX="26.0" layoutY="297.0" mnemonicParsing="false" onAction="#addingToTable" prefHeight="50.0" prefWidth="120.0" style="-fx-background-color: #23e01f;" text="Добавить">
            <font>
                <Font name="System Bold" size="18.0" />
            </font></Button>
        <Label fx:id="label2" layoutX="18.0" layoutY="65.0" text="Название блюда">
            <font>
                <Font name="System Bold" size="14.0" />
            </font></Label>
        <Label fx:id="label1" layoutX="18.0" layoutY="29.0" text="ФИО Сотрудника">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font></Label>
        <TextField fx:id="FIOCoworker" layoutX="186.0" layoutY="25.0" />
        <TextField fx:id="nameBluda" layoutX="186.0" layoutY="63.0" />
        <Label fx:id="label3" layoutX="18.0" layoutY="108.0" text="Дата">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
        </Label>
      <DatePicker fx:id="date" layoutX="186.0" layoutY="106.0" />
      <Label fx:id="label4" layoutX="18.0" layoutY="154.0" text="Время">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="time" layoutX="186.0" layoutY="151.0" />
      <Label fx:id="label5" layoutX="17.0" layoutY="201.0" text="Номер столика">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="numberTable" layoutX="186.0" layoutY="198.0" />
      <Label fx:id="label6" layoutX="17.0" layoutY="244.0" text="Количество заказов">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="count" layoutX="186.0" layoutY="242.0" />
      <Button fx:id="update" layoutX="300.0" layoutY="297.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="120.0" style="-fx-background-color: #17abb3;" text="Изменить">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
    </children>
</AnchorPane>
